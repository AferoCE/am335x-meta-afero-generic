[Unit]
Description = WPA Supplicant
After = network.target

[Service]
Type=simple
ExecStart=/usr/sbin/wpa_supplicant -e/usr/share/wl18xx/entropy.bin -iwlan0 -Dnl80211 -c/usr/share/wl18xx/wpa_supplicant.conf
ExecStartPre=/usr/bin/start_wifi
Restart=always
RestartSec=2

[Install]
WantedBy = multi-user.target

